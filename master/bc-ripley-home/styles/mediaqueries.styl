		.menu12
			li
				width 50%
				.text
					font-size 10px
		.menu13
			li
				width 50%
				.text
					font-size 9px
				&:first-child
					width 100%

		.menu6, .menu8, .menu10, .menu12
			li
				&:nth-child(even)
					border-left 1px solid #000
		.menu7, .menu9, .menu11, .menu13
			li
				&:nth-child(even)
					border-right 1px solid #000
		.menu7, .menu8, .menu9, .menu10, .menu11, .menu12
			display flex !important
			flex-wrap wrap
			flex-basis 100%
		
		.menu8, .menu9, .menu10, .menu11, .menu12, .menu13
			height calc(100vh - 50px)
			display flex !important
			flex-wrap wrap
			flex-basis 100%
			li
				a
					height 100% !important

		.collapse-movil
			&.menu-white
				bgcolor(white)
				.current-category
					color black
				.menu
					border-top 1px solid black
					background-color white !important
					li
						border-color black
				.text
					color black
			&.menu-black
				bgcolor(black)
				.current-category
					color white
				.menu
					border-top 1px solid white
					background-color black !important
					li
						border-color white
				.text
					color white

		.slick-dots
			bottom 5px

