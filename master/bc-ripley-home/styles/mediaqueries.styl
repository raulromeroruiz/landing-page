			top 0
			right 0
			margin-top 0
			z-index 2
			display flex
			align-items center

		.category
			.text
				width 100%
				padding 0
				margin 0
				&:after 
					display none
				br
					display none
			&.active
				&::before
					display none
		.category:hover, .active 
			background-color transparent

		#main-menu
			padding-left 0
			padding-right 0
			// display flex
			// align-items center
			// justify-content center
			// flex-direction row
			// flex-wrap wrap
			// flex-basis 100%
			li
				display flex
				align-items center
				justify-content center
				// width 50%
				float left
				clear none
				// clear both
				&:nth-child(1), &:nth-child(2)
					// border-top 1px solid #fff
				&:nth-child(even)
					// border-left 1px solid #000
				&:last-child
					margin 0 auto
					float none
					// border none
				a
					display table-cell
					vertical-align middle
					line-height normal
					font-size 3.5vw
					text-align center

					height 42px
					display flex
					align-items center
					justify-content center
					// padding 0 20px
				&:nth-child(20)
					border-top none
					border-left none
				&:nth-child(21)
					border-left none
		.menu
			margin-right 0
			margin-left 0
			width 100%
			clear both
			overflow hidden
			background-color white
			box-shadow none
			overflow hidden
			border-radius 0
			height auto
			bottom auto
			// padding-top 40px
			// border-top 1px solid #000
			// border-left 1px solid #000
			top -400px
			position absolute
			li
				float left
				clear none
				border-bottom 1px solid #000
				// border 1px solid #000
				width 50%
				&:nth-child(1)
					// border-top 1px solid #fff
				&:nth-child(11), &:nth-child(12)
					border-bottom none
				&:nth-child(even)
					// border-left 1px solid #000
				a
					display block
					color black
					height auto
					.icon
						display none
				&:after
					display none
		.menu4
			li
				width 100%
		.menu5
			li
				width 100%
		.menu6
			li
				width 50%
		.menu7
			li
				width 50%
				&:first-child
					width 100%
		.menu8
			li
				width 50%
		.menu9
			li
