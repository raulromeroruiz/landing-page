header.ripley-header
	.message-area
		.message-area-content
	.marketing-ribbon
		.marketing-ribbon-fixed
			style.
				.cintcentro1 {
				display: none;
				}
				.cintdesktop, .cintmobile {
				display: block;
				}
				.marketing-ribbon-fixed, .marketing-ribbon {
				height: 56px;
				}
				@media (max-width: 767px){
				.marketing-ribbon-fixed, .marketing-ribbon {
					height: 48px;
				}
				}
				@media (min-width: 768px) and (max-width: 1024px) {
				.cintdesktop {
				display: none;
				}
				.cintcentro1 {
				display: block;
				}
				}
			// Desktop
			a.hidden-sm-down.cintdesktop(href='http://simple.ripley.com.pe/tv-y-video/ofertas/big-sale-tv', style='height: 56px;background: url(http://www.ripley.com.pe/minisitios/home/cintillo/17/02/23/v2/cintillo1.jpg) white no-repeat center top', target='_top')
			// Mobile
			a.hidden-md-up.cintmobile(href='http://simple.ripley.com.pe/tv-y-video/ofertas/big-sale-tv', style='height: 48px;background: url(http://www.ripley.com.pe/minisitios/home/cintillo/17/02/23/v2/cintillom1.jpg) white no-repeat center top', target='_top')
			// 1024px
			a.cintcentro1(href='http://simple.ripley.com.pe/tv-y-video/ofertas/big-sale-tv', style='height: 56px;background: url(http://www.ripley.com.pe/minisitios/home/cintillo/17/02/23/v2/cintillot1.jpg) white no-repeat center top', target='_top')

